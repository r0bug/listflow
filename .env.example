# Server Configuration
PORT=3001
NODE_ENV=development

# Segmind AI Configuration
SEGMIND_API_KEY=your_segmind_api_key_here
SEGMIND_MODEL=llava-v1.6
# Model options: llava-v1.6, claude-3.7-sonnet, claude-3-opus

# eBay API Configuration
EBAY_SANDBOX=true
EBAY_APP_ID=your_ebay_app_id_here
EBAY_CERT_ID=your_ebay_cert_id_here
EBAY_DEV_ID=your_ebay_dev_id_here
EBAY_AUTH_TOKEN=your_ebay_auth_token_here

# PayPal Configuration
PAYPAL_EMAIL=your_paypal_email@example.com

# Seller Information
SELLER_POSTAL_CODE=10001

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/consoleebay

# Redis Configuration (for Bull queue)
REDIS_URL=redis://localhost:6379

# Frontend URL
CLIENT_URL=http://localhost:5173

# Security
JWT_SECRET=your_jwt_secret_here
SESSION_SECRET=your_session_secret_here